<% content_for :head do %>
  <% stylesheet 'report'%>
<% end %>

<table id="report_view_table">
    <tr>
      <th class="list_header" colspan="3">
        <h2>Recent updates for: <%= @location.name %></h2>
      </th>
    </tr>
    <tbody id = "all_report_items">
		<%= render :partial => "report_items", :collection => @report_items %>
    </tbody>
</table>
<% form_tag(:action => :display_report_items, :id => @location.id, :more_time => @start_time.to_datetime) do %>
<br />  
<div><%= submit_tag 'View More' %></div>

<% end -%>