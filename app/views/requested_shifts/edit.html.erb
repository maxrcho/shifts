<h1>Editing your shift request</h1>

<% form_for(@requested_shift) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @requested_shift %> |
<%= link_to 'Back', requested_shifts_path %>
